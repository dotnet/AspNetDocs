{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.exe",
          "**/*.wmv",
          "**/*.zip"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "_navPath": "/foo",
      "_navRel": "/foo",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "ms.prod": "aspnet-framework",
      "ms.topic": "conceptual",
      "uhfHeaderId": "MSDocsHeader-AspNet",
      "searchScope": [
        "ASP.NET"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.aspnet-conceptual",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {
      "ms.technology": {
        "ajax/**/**.md": "aspnet-ajax",
        "aspnet/**/**.md": "aspnet-aspnet",
        "identity/**/**.md": "aspnet-identity",
        "mobile/**/**.md": "aspnet-mobile",
        "mvc/**/**.md": "aspnet-mvc",
        "signalr/**/**.md": "aspnet-signalr",
        "single-page-application/**/**.md": "aspnet-spa",
        "visual-studio/**/**.md": "aspnet-visualstudio",
        "web-api/**/**.md": "aspnet-webapi",
        "web-forms/**/**.md": "aspnet-webforms",
        "web-pages/**/**.md": "aspnet-webpages",
        "webhooks/**/**.md": "aspnet-webhooks",
        "whitepapers/**/**.md": "aspnet-whitepapers"
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
  "fileMetadata": {
     "searchScope": { 
       "contribute/**.md": ["help-docs","Contributor"],
       "contribute/**.yml": ["help-docs","Contributor"]       
     },    
     "recommendations": { 
       "contribute/**.md": true,
       "onboard/**.md": true,
       "localization/**.md": false  
     }
    }
}
