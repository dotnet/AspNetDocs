{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.exe",
          "**/*.wmv",
          "**/*.zip"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "Standard",
      "feedback_help_link_type": "get-help-at-qna",
      "feedback_help_link_url": "https://learn.microsoft.com/answers/tags/174/aspnet",
      "_navPath": "/foo",
      "_navRel": "/foo",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json",
      "recommendations": true,
      "ms.prod": "aspnet-framework",
      "ms.topic": "conceptual",
      "uhfHeaderId": "MSDocsHeader-AspNet",
      "searchScope": [
        "ASP.NET"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.aspnet-conceptual",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {
      "ms.subservice": {
        "aspnet/**/**.md": "overview",
        "single-page-application/**/**.md": "spa",
        "signalr/**/**.md": "signalr",
        "mobile/**/**.md": "mobile",
        "web-api/**/**.md": "web-api",
        "web-pages/**/**.md": "web-pages",
        "identity/**/**.md": "identity",
        "webhooks/**/**.md": "web-hooks",
        "web-forms/**/**.md": "web-forms",
        "ajax/**/**.md": "ajax",
        "visual-studio/**/**.md": "visual-studio",
        "mvc/**/**.md": "mvc",
        "whitepapers/**/**.md": "whitepapers"
      },
      "ms.technology": {
      },
      "recommendations": {
        "mvc/overview/getting-started/introduction/*.md": false
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
